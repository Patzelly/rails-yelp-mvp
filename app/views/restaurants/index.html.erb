<h1>Restaurants</h1>
<h3><%= link_to "Create a new restaurant", new_restaurant_path %></h3>

<div class="container">
  <div class="row">
    <% @restaurants.each do |restaurant| %>
    <div class="col-xs-12 col-sm-4">
        <%= render 'card', restaurant: restaurant  %>
    </div>
    <% end %>
  </div>
</div>
